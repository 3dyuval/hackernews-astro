---
import Link from './feedLinkItem.astro'
import client, { gql } from '../GraphQLClient'

const {data, error} = await client.query(gql`
query FeedQuery {
		feed {
			id
			url
			description
			createdAt
		}
	}
`, {}).toPromise()

if (error) {
	console.error(error)
}

---


{data.feed.map(i => {
	return <Link {...i} />
})}
