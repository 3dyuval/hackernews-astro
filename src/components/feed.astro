---
import LinkBody from './link-body.astro'
import client, { gql } from '../GraphQLClient'

const {data, error} = await client.query(gql`
query FeedQuery {
		feed {
			id
			url
			description
			createdAt
		}
	}
`, {})

if (error) {
	console.error(error)
}

---

<ul>

	{data.feed.map((link, index) => {
		return <li><LinkBody link={link} index={index}  /></li>
	})}

</ul>